<?xml version="1.0" encoding="UTF-8"?>
<container-wizard-conf xmlns="http://www.rsicms.com/rsuite/ns/conf/container-wizard" xmlns:r="http://www.rsuitecms.com/rsuite/ns/metadata" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Audit Report Product Configuration" r:rsuiteId="9927" xsi:schemaLocation="http://www.rsicms.com/rsuite/ns/conf/container-wizard containerWizardConf.xsd">
    
    <pages r:rsuiteId="9929">
        
        <page api-name="gao.newblue.webservice.CreateProductByType" form-id="gao-newblue-form-select-product-type" sub-pages="0"/>
        <page api-name="rsuite-container-wizard-plugin-ws-invoke-wizard" form-id="gao-newblue-form-create-product-page-1" sub-pages="0"/>
        <!--<page form-id="sample.contents.page" sub-pages="0"/>-->
        <page action-id="rsuite-container-wizard-plugin:wizardPage" api-name="rsuite-container-wizard-plugin-ws-invoke-wizard" sub-pages="1"/>
        <page api-name="rsuite-container-wizard-plugin-ws-invoke-wizard" form-id="gao-newblue-form-create-product-page-3" sub-pages="0"/>
        
    </pages>
    
    <primary-container acl-id="non-feedback-containers" default-acl-id="master-sections" r:rsuiteId="9930" type="product">
        
        <container-conf acl-id="feedback-container" name="Reviewer Feedback" type="feedback"/>
        
        <xml-mo-conf multiple="0" name="Cover" required="1" template-lmd-value="AuditReportCover"/>
        <xml-mo-conf multiple="0" name="Abbreviations" required="0" template-lmd-value="AuditReportAbbreviations"/>
        <xml-mo-conf multiple="0" name="Introduction" required="1" template-lmd-value="AuditReportIntroduction"/>
        <xml-mo-conf multiple="0" name="Background" required="1" template-lmd-value="AuditReportBackground"/>
        <xml-mo-conf multiple="1" name="Finding" required="1" template-lmd-value="AuditReportFinding"/>
        <xml-mo-conf multiple="0" name="Conclusions" required="0" template-lmd-value="AuditReportConclusions"/>
        <xml-mo-conf multiple="0" name="Recommendations" required="0" template-lmd-value="AuditReportRecommendations"/>
        <xml-mo-conf multiple="1" name="Agency Comments" required="1" template-lmd-value="AuditReportAgencyComments"/>
        <xml-mo-conf multiple="0" name="Closing" required="0" template-lmd-value="AuditReportClosing"/>
        <xml-mo-conf multiple="1" name="Appendix" required="0" template-lmd-value="AuditReportAppendix"/>
        <!--<xml-mo-conf name="Related Product" template-lmd-value="AuditReportRelatedProduct" required="0" multiple="1"/>-->
        
        <container-conf acl-id="non-feedback-containers" name="images" type="folder"/>
        
    </primary-container>
    
    <acls r:rsuiteId="9931">
        <acl id="non-feedback-containers">
            <ace content-permissions="list, view, edit" project-role="Authors"/>
            <ace content-permissions="list, view" project-role="Reviewers"/>
            <ace content-permissions="list, view" project-role="Referencers"/>
        </acl>
        <acl id="feedback-container">
            <ace content-permissions="list, view" project-role="Authors"/>
            <ace content-permissions="list, view, edit" project-role="Reviewers"/>
            <ace content-permissions="" project-role="Referencers"/>
        </acl>
        <acl id="master-sections">
            <ace content-permissions="list, view, edit, delete" project-role="Authors"/>
            <ace content-permissions="list, view" project-role="Reviewers"/>
            <ace content-permissions="list, view" project-role="Referencers"/>
        </acl>
    </acls>
    
</container-wizard-conf>